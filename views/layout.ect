<!DOCTYPE html>
<html>
    <head>
        <title>
            <%- @title %>
        </title>
        <link href="/sass-style.css" rel="stylesheet" type="text/css">
        </link>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/basket.js/0.5.2/basket.full.min.js">
        </script>
        <% if process.env.NODE_ENV is 'development' : %>
        <script type="text/javascript">
            basket.clear();
        </script>
        <% end %>
        <script type="text/javascript">
            //javascript files
                basket.require(
                    //{ url: '' },
                    { url: 'https://code.jquery.com/jquery-3.1.0.slim.min.js'}
                );
        </script>
    </head>
    <body>
        <% include 'header' %>
        <% content %>
        <% include 'footer' %>
    </body>
</html>
